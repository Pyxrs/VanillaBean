{
  "schemaVersion": 1,
  "id": "vanillabean",
  "version": "${version}",

  "name": "VanillaBean",
  "description": "VanillaBean is a mod that focuses on rebalancing Minecraft to add extra challenge in an alpha-like manner without sacrificing modern updates.",
  "authors": [
    "SimplyCmd"
  ],
  "contact": {
    "homepage": "https://simplycmd.github.io/vanillabean",
    "sources": "https://github.com/SimplyCmd/VanillaBean"
  },

  "license": "MIT",
  "icon": "assets/vanillabean/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.simplycmd.vanillabean.Main"
    ],
    "client": [
      "io.github.simplycmd.vanillabean.Client"
    ]
  },
  "mixins": [
    "vanillabean.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
  }
}
